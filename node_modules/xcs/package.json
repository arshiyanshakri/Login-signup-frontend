{
  "name": "xcs",
  "description": "eXtremely awesome AWS ECS cli",
  "version": "0.0.0",
  "author": "Alex Puschinsky @alexpusch",
  "bin": {
    "xcs": "./bin/run"
  },
  "bugs": "https://github.com/alexpusch/xcs/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.8.8",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-help": "^2.1.3",
    "aws-sdk": "^2.344.0",
    "fs-extra": "^7.0.0",
    "prettyjson": "^1.2.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "globby": "^8.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/alexpusch/xcs",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "xcs",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "alexpusch/xcs",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
